{
    "workbench.colorTheme": "Ayu Mirage",
    "editor.acceptSuggestionOnEnter": "off",
    "security.workspace.trust.untrustedFiles": "open",
    "rust-analyzer.lens.implementations.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "workbench.iconTheme": "eq-material-theme-icons",
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.closingBraceHints.enable": false,
    "rust-analyzer.hover.actions.run.enable": false,
    "rust-analyzer.lens.run.enable": false,
    "rust-analyzer.hover.actions.debug.enable": false,
    "rust-analyzer.lens.debug.enable": false,
    "go.lintOnSave": "off",
    "go.buildOnSave": "off",
    "go.coverOnTestPackage": false,
    "go.enableCodeLens": {
        "runtest": false
    },
    "go.languageServerExperimentalFeatures": {},
    "go.lintTool": "golangci-lint",
    "go.playground": {
        "openbrowser": false,
        "share": false,
        "run": false
    },
    "go.survey.prompt": false,
    "go.testExplorer.enable": false,
    "go.testExplorer.concatenateMessages": false,
    "go.testExplorer.showOutput": false,
    "go.vetOnSave": "off",
    "editor.formatOnSaveMode": "modifications",
    "[go]": {
        "editor.formatOnSave": false
    },
    "telemetry.telemetryLevel": "off",
    "liveshare.accessibility.soundsEnabled": false,
    "liveshare.accessibility.soundVolume": 0,
    "liveshare.accessibility.voiceVolume": 0,
    "liveshare.populateGitCoAuthors": "never",
    "editor.fontFamily": "Liberation Mono",
    "vscode-neovim.mouseSelectionStartVisualMode": true,
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "workbench.enableExperiments": false,
    "editor.fontSize": 14,
    "haskell.ghcupExecutablePath": "~/.ghcup/bin/ghcup",
    "haskell.manageHLS": "GHCup"
}

